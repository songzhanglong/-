/* pages/label/label.wxss */
page{
    background-color: rgb(251, 249, 254);
}

.label-demo {
    padding-left: 10px;
    padding-right: 10px;
}

.section{
    width: 100%;
    margin-bottom: 20px;
}

.label-1__text,.label-2__text{
    display: inline-block;
    vertical-align: middle
}

.label-1__icon,.label-2__icon{
    position: relative;
    display: inline-block;
    vertical-align: middle;
    margin-right: 10px;
    width: 18px;
    height: 18px;
    background-color: #fcfff4;
    border-radius: 50px;
}

